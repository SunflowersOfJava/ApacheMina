package com.boonya.mina.protocol.sms.bean;
  
option java_package = "com.boonya.mina.protocol.sms.bean";
option java_outer_classname = "Sms";
  
message Sms {
  required string protocol         = 1;
  required string sender       = 2;
  required string receiver         = 3;
  required string content      = 4;
}


